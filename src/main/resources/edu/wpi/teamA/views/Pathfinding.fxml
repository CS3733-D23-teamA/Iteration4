<?xml version="1.0" encoding="UTF-8"?>

<?import io.github.palexdev.materialfx.controls.MFXButton?>
<?import io.github.palexdev.materialfx.controls.MFXFilterComboBox?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.Line?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>
<?import net.kurobako.gesturefx.GesturePane?>

<BorderPane stylesheets="@stylesheets/main.css" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.wpi.teamA.controllers.Navigation.PathfindingController">
   <top>
      <fx:include fx:id="navbar" source="NavigationBar.fxml" BorderPane.alignment="TOP_CENTER" />
   </top>
   <center>
      <StackPane alignment="TOP_CENTER" BorderPane.alignment="CENTER_RIGHT">
         <children>
            <GesturePane fx:id="gesturePane" />
            <VBox alignment="CENTER" fillWidth="false" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="350.0" prefWidth="100.0" spacing="4.0" StackPane.alignment="TOP_RIGHT">
               <children>
                  <MFXButton fx:id="levelL1Button" prefHeight="60.0" prefWidth="60.0" style="-fx-background-color: #012D5A; -fx-text-fill: #f1f1f1; -fx-font-weight: bold;" text="L1" textFill="#012d5a">
                     <font>
                        <Font name="Open Sans Regular" size="24.0" />
                     </font>
                  </MFXButton>
                  <MFXButton fx:id="levelL2Button" prefHeight="60.0" prefWidth="60.0" style="-fx-background-color: #012D5A; -fx-text-fill: #f1f1f1; -fx-font-weight: bold;" text="L2" textFill="#012d5a">
                     <font>
                        <Font name="Open Sans Regular" size="24.0" />
                     </font>
                  </MFXButton>
                  <MFXButton fx:id="level1Button" prefHeight="60.0" prefWidth="60.0" style="-fx-background-color: #012D5A; -fx-text-fill: #f1f1f1; -fx-font-weight: bold;" text="1" textFill="#012d5a">
                     <font>
                        <Font name="Open Sans Regular" size="24.0" />
                     </font>
                  </MFXButton>
                  <MFXButton fx:id="level2Button" prefHeight="60.0" prefWidth="60.0" style="-fx-background-color: #012D5A; -fx-text-fill: #f1f1f1; -fx-font-weight: bold;" text="2" textFill="#012d5a">
                     <font>
                        <Font name="Open Sans Regular" size="24.0" />
                     </font>
                  </MFXButton>
                  <MFXButton fx:id="level3Button" prefHeight="60.0" prefWidth="60.0" style="-fx-background-color: #012D5A; -fx-text-fill: #f1f1f1; -fx-font-weight: bold;" text="3" textFill="#012d5a">
                     <font>
                        <Font name="Open Sans Regular" size="24.0" />
                     </font>
                  </MFXButton>
               </children>
            </VBox>
         </children>
         <BorderPane.margin>
            <Insets />
         </BorderPane.margin>
      </StackPane>
   </center>
   <left>
      <ScrollPane fitToHeight="true" fitToWidth="true" BorderPane.alignment="CENTER_LEFT">
         <content>
            <VBox alignment="TOP_CENTER" fillWidth="false" maxWidth="-Infinity" minWidth="-Infinity" prefWidth="350.0" spacing="50.0" style="-fx-background-color: #012D5A;">
               <children>
                  <Label id="title" fx:id="title1" alignment="CENTER" style="-fx-text-fill: #f1f1f1;" text="Find Path" textAlignment="CENTER">
                     <font>
                        <Font name="Open Sans Regular" size="24.0" />
                     </font>
                  </Label>
                  <Line endX="100.0" startX="-200.0" stroke="#f1f1f1" strokeLineCap="BUTT" strokeWidth="3.0" styleClass="line" />
                  <VBox alignment="CENTER_LEFT" spacing="20.0">
                     <children>
                        <VBox spacing="5.0">
                           <children>
                              <Text strokeType="OUTSIDE" strokeWidth="0.0" style="-fx-fill: #f1f1f1;" text="Starting Location" wrappingWidth="181.9421045763973">
                                 <font>
                                    <Font size="24.0" />
                                 </font>
                              </Text>
                              <MFXFilterComboBox fx:id="startSelection" onAction="#checkPath" prefHeight="44.0" prefWidth="300.0">
                                 <padding>
                                    <Insets left="10.0" />
                                 </padding>
                              </MFXFilterComboBox>
                           </children>
                        </VBox>
                        <VBox spacing="5.0">
                           <children>
                              <Text strokeType="OUTSIDE" strokeWidth="0.0" style="-fx-fill: #f1f1f1;" text="Destination" wrappingWidth="125.55863594846761">
                                 <font>
                                    <Font size="24.0" />
                                 </font>
                              </Text>
                              <MFXFilterComboBox fx:id="endSelection" onAction="#checkPath" prefHeight="44.0" prefWidth="300.0" />
                           </children>
                        </VBox>
                        <VBox fx:id="searchAlgorithmVbox" spacing="5.0">
                           <children>
                              <Text fx:id="searchAlgorithmText" strokeType="OUTSIDE" strokeWidth="0.0" style="-fx-fill: #f1f1f1;" text="Search Algorithm" wrappingWidth="194.5586359484676">
                                 <font>
                                    <Font size="24.0" />
                                 </font>
                              </Text>
                              <MFXFilterComboBox fx:id="searchAlgorithmSelection" onAction="#checkPath" prefHeight="44.0" prefWidth="300.0" />
                           </children>
                        </VBox>
                     </children>
                  </VBox>
                  <VBox maxWidth="300.0">
                     <children>
                        <Label id="title" fx:id="title" style="-fx-text-fill: #f1f1f1;" text="Directions:">
                           <font>
                              <Font name="Open Sans Regular" size="24.0" />
                           </font>
                           <padding>
                              <Insets bottom="5.0" />
                           </padding>
                        </Label>
                        <VBox style="-fx-background-color: bwhite; -fx-background-radius: 10;">
                           <children>
                              <Text fx:id="directions" fill="#8d8989" strokeType="OUTSIDE" strokeWidth="0.0" text="Please enter starting and ending destination" wrappingWidth="300.0" />
                              <Text fx:id="searchAlgorithmTextDirections" fill="#8d8989" strokeType="OUTSIDE" strokeWidth="0.0" text="and select the desired search algorithm" wrappingWidth="300.0">
                                 <VBox.margin>
                                    <Insets top="5.0" />
                                 </VBox.margin></Text>
                           </children>
                           <padding>
                              <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                           </padding>
                        </VBox>
                     </children>
                  </VBox>
               </children>
               <padding>
                  <Insets bottom="50.0" left="10.0" right="10.0" top="50.0" />
               </padding>
            </VBox>
         </content>
      </ScrollPane>
   </left>
</BorderPane>
